package com.demo;

public class SortArray {

	public static void main(String[] args) {
		int[] a= {0,5,3,1,9};
		for(int i=0;i<a.length;i++) {
			for(int j=i+1;j<a.length;j++) {
				//Smallest to largest
				//compaire values to next index 
				if(a[i]>a[j]) {
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;
					
				}
			}
		}
		
		for(int k=0;k<a.length;k++) {
			System.out.println(a[k]);
		}

	}

}
